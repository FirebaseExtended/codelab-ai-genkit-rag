{
  "name": "compass",
  "main": "src/index.ts",
  "engines": {
    "node": "20 || 22 || 24"
  },
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "build": "next build && cp -r ./prompts ./.next/prompts",
    "build:clean": "rm -rf ./next",
    "dev": "genkit start -- npx nodemon --ext prompt --watch prompts --exec next dev",
    "dev:next": "next dev",
    "dev:genkit": "genkit start -- npx nodemon --ext prompt --watch prompts --exec \"tsx --watch src/index.ts\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@genkit-ai/firebase": "^1.11.1",
    "@genkit-ai/vertexai": "^1.11.1",
    "clsx": "^2.1.1",
    "genkit": "^1.11.1",
    "next": "15.2.4",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.77.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "genkit-cli": "^1.11.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.3"
  }
}
